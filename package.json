{
  "name": "nc-web-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "mstart": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-prod": "ng build --configuration production --base-href=/browser/",
    "build-staging": "ng build --configuration development --base-href=/development/",
    "dev:ssr": "ng run nc-web-frontend:serve-ssr",
    "serve:ssr": "node dist/nc-web-frontend/server/main.js",
    "build:ssr": "ng build && ng run nc-web-frontend:server",
    "build-prod:ssr": "ng build --configuration production && ng run nc-web-frontend:server",
    "build-staging:ssr": "ng build --configuration staging --base-href=/development/ && ng run nc-web-frontend:server",
    "prerender": "ng run nc-web-frontend:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.8",
    "@angular/common": "^14.2.8",
    "@angular/compiler": "^14.2.8",
    "@angular/core": "^14.2.8",
    "@angular/forms": "^14.2.8",
    "@angular/platform-browser": "^14.2.8",
    "@angular/platform-browser-dynamic": "^14.2.8",
    "@angular/platform-server": "^14.2.8",
    "@angular/router": "^14.2.8",
    "@angular/service-worker": "^14.0.0",
    "@nguniversal/express-engine": "^14.2.0",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.2.2",
    "bootstrap-icons": "^1.9.1",
    "express": "^4.15.2",
    "hamburgers": "^1.2.1",
    "jquery": "^3.6.0",
    "ngx-toastr": "^15.2.0",
    "popper.js": "^1.16.1",
    "rxjs": "~7.5.0",
    "service-worker": "^0.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular/cli": "~14.0.3",
    "@angular/compiler-cli": "^14.2.8",
    "@nguniversal/builders": "^14.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.7.2"
  }
}